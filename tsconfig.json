{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"strictNullChecks": true,
		"target": "ES2022",
		"module": "CommonJS",
		"moduleResolution": "Node10",
		"outDir": "dist",
		"declaration": true,
		"isolatedModules": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
